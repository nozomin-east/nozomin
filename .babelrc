{
  "plugins": [
    [
      "react-css-modules", {
        "generateScopedName": "[path]___[name]__[local]___[hash:base64:5]",
        "handleMissingStyleName": "warn",
        "filetypes": {
          ".scss": {
            "syntax": "postcss-scss",
            "plugins": [
              "postcss-nested"
            ]
          }
        }
      }
    ],
  ]
}
